#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules/@commitlint/cli/node_modules:/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules/@commitlint/node_modules:/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules:/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules/@commitlint/cli/node_modules:/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules/@commitlint/node_modules:/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules:/home/sakar/n8n/n8n-deployment-docker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules/@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/@commitlint+cli@19.8.0_@types+node@22.14.0_typescript@5.8.3/node_modules/@commitlint/cli/cli.js" "$@"
fi
